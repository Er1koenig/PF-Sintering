function laplace = Laplace(Set,i,j,dx,dy)

iplus = i+1;
iminus = i-1;
jplus = j+1;
jminus = j-1;

if (i == 1)
   iminus = length(Set);
end

if (i == length(Set))
   iplus = 1; 
end

if (j == 1)
   jminus = length(Set);
end

if (j == length(Set))
   jplus = 1; 
end

laplaceX = ((Set(iplus,j)-Set(i,j))-(Set(i,j)-Set(iminus,j)))/(dx^2);
laplaceY = ((Set(i,jplus)-Set(i,j))-(Set(i,j)-Set(i,jminus)))/(dy^2);

laplace = laplaceX + laplaceY;

end
